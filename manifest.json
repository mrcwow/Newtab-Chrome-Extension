{
    "manifest_version": 3,
    "name": "Newtab.G",
    "description": "Change newtab to Google Chrome newtab even you use other search engines.",
    "version": "1.1",

    "permissions": ["tabs"],

    "background": {
      "service_worker": "background.js"
    },

    "icons": {
      "16": "assets/icons/icon_16.png",
      "48": "assets/icons/icon_48.png",
      "128": "assets/icons/icon_128.png"
	  }
}